    "@types/jest": "^29.5.2",
    "eslint": "^8.42.0",
    "eslint-config-base": "workspace:*",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "regenerator-runtime": "^0.13.11",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
