{
  "name": "core",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "jest",
    "dev": "nodemon",
    "coverage": "jest --coverage",
